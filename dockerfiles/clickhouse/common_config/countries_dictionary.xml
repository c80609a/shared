<yandex>
  <include_from>/etc/clickhouse-server/substitution.xml</include_from>
  <dictionary>
    <name>countries</name>
    <source>
      <mysql incl="mysql_conf">
        <table>countries</table>
      </mysql>
    </source>
    <lifetime>
      <min>300</min>
      <max>360</max>
    </lifetime>
    <layout>
      <flat/>
    </layout>
    <structure>
      <id>
        <name>id</name>
      </id>
      <attribute>
        <name>name</name>
        <type>String</type>
        <null_value>Undefined</null_value>
      </attribute>
      <attribute>
        <name>short_name</name>
        <type>String</type>
        <null_value>UNDEFINED</null_value>
      </attribute>
    </structure>
  </dictionary>
</yandex>
